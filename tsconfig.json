{
  "compilerOptions": {
    "strictNullChecks": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "node"
    ],
    "baseUrl": "./",
    "paths": {
      "@generated": [
        "./dist/generated/index.ts"
      ],
      "@frontend/*": [
        "./src/frontend/*"
      ]
    }
  }
}