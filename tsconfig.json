{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "node",
      "jest"
    ],
    "baseUrl": "./",
    "paths": {
      "@generated": "./dist/generated/index.ts",
      "@frontend/*": "./src/frontend/*"
    }
  }
}